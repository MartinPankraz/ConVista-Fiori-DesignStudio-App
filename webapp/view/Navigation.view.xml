<mvc:View controllerName="convista.com.arp.demo.controller.Navigation" xmlns:html="http://www.w3.org/1999/xhtml"  xmlns:mvc="sap.ui.core.mvc" xmlns:core="sap.ui.core"
	xmlns="sap.m" xmlns:l="sap.ui.layout" xmlns:tnt="sap.tnt" height="100%">
	<App id="app" class="ccBG"><!-- id="app" left out intentionally for bug tracking purpose-->
		<pages>
			<Page 
				  id="homePage"
				  showHeader="false"
				  title="{i18n>title}"
				  showNavButton="true"
				  enableScrolling="false">
				<content>
					<l:HorizontalLayout id="layout_horizontal_sideNav" class="ccHorizontalSideNavCon">
			            <tnt:NavigationList id="navigationList"
			            					itemSelect="onNavListItemSelect"
			            					expanded="false"
			            					class="ccTntSideNav">
		                </tnt:NavigationList>
		                <l:VerticalLayout id="layout_vartical_Nav" class="ccVerticalNavCon">
			                <TabContainer
								id="myTabCon"
								width="100%"
								showAddNewButton="false"
								itemClose="tabItemCloseHandler"
								itemSelect="tabItemSelectHandler"
								class="ccTabContainer">
			                	<items>
			                		<TabContainerItem id="firstItem" name="ARP Home" key="landing_tabItem">
			                			<content><Text text="/NavigationItems/1"/></content>
			                		</TabContainerItem>
			                	</items>
			                </TabContainer>
			                <NavContainer
								id="myNavCon"
								width="100%"
								height="100%"
								class="ccNavCon">
			                	<Page
									id="firstItem_page"
									title="Home Page"
									showHeader="false"
									showSubHeader="false"
									class="myPageOverflow"
									showNavButton="false">
			                		<core:HTML id="firstItem_page_html" height="100%"/>
								</Page>
			                </NavContainer>
		                </l:VerticalLayout>
					</l:HorizontalLayout>
				</content>
				<footer>
					<OverflowToolbar id="footerBar">
						<Text id="companyText" class="sapUiSmallMarginBeginEnd ccCompanyText" text="2017 ConVista Consulting AG"/>
						<ToolbarSeparator />
						<Link id="contactUs" class="sapUiSmallMarginBeginEnd ccContactUsText" text="Contact Us" press="onContactUsClicked"/>
						<ToolbarSeparator />
						<Image press="onTwitterIconPressed" src="data:image/png;base64,"></Image>
						<Image press="onFacebookIconPressed" src="data:image/png;base64,"></Image>
						<Image press="onLinkedInIconPressed" src="data:image/png;base64,"></Image>
						<ToolbarSpacer />
					</OverflowToolbar>
				</footer>	
			</Page>
		</pages>
	</App>
</mvc:View>